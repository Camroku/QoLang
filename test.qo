func: print_text[whichone]{
  if (whichone == 1) { | note the ". "1" and 1 are not the same. |
    println("Hello, world!");
  } else if (whichone == 2) {
    println("Hello, QoLang!");
  } else if (whichone == 3) {
    println("Hello, Camroku.TECH!");
  } else {
    println("Invalid input!");
  };
};

